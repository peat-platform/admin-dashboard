extends main-container

block header
	include ./header-navbar.jade

block content
	<div id="typesmain" class="wrapper">
	<div class="column-left" style="float: left; width: 30%; height:200px; overflow: auto;">
	<h3>User Accounts</h3>
	ul
		each cid in cloudlets
			li
				a(href="#")= cid
	</div>
	<div class="column-middle" style="float: left; width: 30%;height:200px; overflow: auto;">
	<h3>Types</h3>
	<ul id="type_id_list"></ul>
	</div>
	<div class="column-right" style="float: left; width: 30%;height:200px; overflow: auto;">
	<h3>Objects</h3>
	<ul id="object_id_list"></ul>
	</div>

	<div id="data_container" style="clear:both; width:100%; display:none">
	<h3>Data Record</h3>
	<pre id="data"></pre>
	<h3>Meta Data</h3>
	<pre id="meta_data"></pre>
	</div>
	script(type='text/javascript', src="/admin/javascripts/md5.js")
	script(type='text/javascript', src="/admin/javascripts/data.js")
	link(rel='stylesheet', href='/admin/stylesheets/data.css')

	<input type="hidden" id="session" name="session" value="#{session}">